{
  "name": "Mini Login Notes",
  "image": "mcr.microsoft.com/devcontainers/javascript-node:20",
  "forwardPorts": [3000],
  "postCreateCommand": "npm install",
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode"
      ]
    }
  },
  "portsAttributes": {
    "3000": {
      "label": "Servidor Web",
      "onAutoForward": "openPreview"
    }
  },
  "remoteUser": "node"
}
